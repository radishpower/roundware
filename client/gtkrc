                image {
                        function = BOX
                        detail = "seekbar"
                        state = NORMAL
                        file = "../images/qgn_plat_seek_bar_toolbar_background_normal.png"
                        border = { 20, 20, 19, 19 }
                }

                image {
                        function = BOX
                        detail = "trough-lower"
                        state = INSENSITIVE
                        file = "../images/qgn_plat_seek_bar_toolbar_shaft_left_disabled.png"
                        border = { 4, 0, 0, 0 }
                }
                image {
                        function = BOX
