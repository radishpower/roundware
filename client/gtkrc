{
  GtkButton::separator-height = 15
  engine "sapwood"
    {
      image
        {
	  function = BOX
	  file = "../images/qgn_plat_column_listbox_header.png"
	  border = { 2, 2, 2, 17 }
	}
      image
        {
	  function = FOCUS
	  file = "../images/qgn_plat_column_listbox_header.png"
	  border = { 2, 2, 2, 17 }
	}
    }
}
