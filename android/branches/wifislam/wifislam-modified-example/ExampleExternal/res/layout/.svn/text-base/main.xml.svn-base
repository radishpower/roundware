<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Hello, Example App" />
  
   
    
    
    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/label_timestamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Timestamp"
                android:textAppearance="?android:attr/textAppearanceSmall" />
        
           <TextView
		        android:id="@+id/pose_timestamp"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:text="timestamp"
		        android:textAppearance="?android:attr/textAppearanceSmall" />
		            
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        
            <TextView
                android:id="@+id/label_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Location"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
		        android:id="@+id/pose_location"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:text="location"
		        android:textAppearance="?android:attr/textAppearanceSmall" />
		   

        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
    
            <TextView
                android:id="@+id/label_position"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Position"
                android:textAppearance="?android:attr/textAppearanceSmall" />
            
            <TextView
		        android:id="@+id/pose_position"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:text="position"
		        android:textAppearance="?android:attr/textAppearanceSmall" />
		    
        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/label_accuracy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Accuracy"
                android:textAppearance="?android:attr/textAppearanceSmall" />      
                
            <TextView
		        android:id="@+id/pose_accuracy"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:text="accuracy"
		        android:textAppearance="?android:attr/textAppearanceSmall" />
		            
        </TableRow>
    </TableLayout>

</LinearLayout>