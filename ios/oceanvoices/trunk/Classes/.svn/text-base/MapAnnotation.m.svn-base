#import "MapAnnotation.h"


@implementation MapAnnotation
@synthesize coordinate;

- (NSString *)subtitle {
	return @"Where you finished recording";
}

- (NSString *)title {
	return @"Your Location";
}

-(id)initWithCoordinate:(CLLocationCoordinate2D) coord {
